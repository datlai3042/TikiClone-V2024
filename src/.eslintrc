{
      "extends": ["react-app", "prettier"],
      "plugins": ["react", "prettier"],
      "rules": {
            "prettier/prettier": [
                  "warn",
                  {
                        "arrowParens": "always",
                        "semi": false,
                        "tabWidth": 6,
                        "endOfLine": "auto",
                        "useTabs": false,
                        "singleQuote": true,
                        "printWidth": 140,
                        "jsxSingleQuote": true
                  }
            ]
      }
}
